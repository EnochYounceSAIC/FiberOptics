    {
      "cell_type": "code",
      "source": [
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "import scipy as sp"
      ],
      "metadata": {
        "id": "y_Y17FE-QlKJ"
      },
      "execution_count": 270,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "length = 3000\n",
        "span = 100\n",
        "#Fiber type: SMF-28\n",
        "#current task: model EDFA noise and amplification for OOK\n",
        "EDFA_noise = 4.7 #dB\n",
        "EDFA_gain = 17 #dBm\n",
        "#fiber data sheet used : https://www.corning.com/media/worldwide/coc/documents/Fiber/product-information-sheets/PI-1470-AEN.pdf\n",
        "#EDFA used EDFA 100s : https://www.thorlabs.com/newgrouppage9.cfm?objectgroup_ID=10680\n",
        "#because OOK, assuming single wavelength light, using 1550nm\n",
        "lambda_light = 1550e-9\n",
        "f_light = 3e8/lambda_light\n",
        "#using max attenuation as a graph or standard deviation is not provided\n",
        "attenuation = 0.17 #dB/km\n",
        "power_initial = 3 #dBm"
      ],
      "metadata": {
        "id": "KEGeQjxPQrA_"
      },
      "execution_count": 271,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "distance = np.arange(0,length,1)\n",
        "# Calculate power in mW\n",
        "# Power starts at initial power, attenuates, and gets a discrete gain at each amplifier location\n",
        "power_mW = np.zeros(length)\n",
        "power_mW[0] = 10**(power_initial/10) # Initial power in mW\n",
        "\n",
        "for i in range(1, length):\n",
        "    # Power at current point is power at previous point minus attenuation over 1 km\n",
        "    power_mW[i] = power_mW[i-1] * 10**(-attenuation / 10)\n",
        "    # Add EDFA gain at amplifier locations\n",
        "    if i > 0 and i % span == 0:\n",
        "        power_mW[i] *= 10**(EDFA_gain/10)\n",
        "\n",
        "\n",
        "# Convert power back to dBm for plotting if desired, or keep in mW for OSNR calculation\n",
        "power_dBm = 10 * np.log10(power_mW)\n",
        "\n",
        "plt.plot(distance,power_dBm)\n",
        "plt.xlabel('distance (km)')\n",
        "plt.ylabel('power (dBm)')\n",
        "plt.title('Power vs Distance of SMF-28 fiber with 1550 nm light with EDFA amplification')\n",
        "plt.show()"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 471
        },
        "id": "D-eYrch7RGu2",
        "outputId": "4a8959af-d9db-48ac-860a-bf4bed94b48d"
      },
      "execution_count": 272,
