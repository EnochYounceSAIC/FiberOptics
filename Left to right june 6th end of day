            "image/png": "\n"
          },
          "metadata": {}
        }
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "# Add a small epsilon to noise to avoid division by zero and log of zero/negative\n",
        "noise_plus_epsilon = noise + np.finfo(float).eps\n",
        "\n",
        "OSNR = 10*np.log10(power_mW[100:]/noise_plus_epsilon[100:])\n",
        "\n",
        "plt.plot(distance[100:],OSNR)\n",
        "plt.xlabel('distance (km)')\n",
        "plt.ylabel('OSNR (dB)')\n",
        "\n",
        "plt.title('OSNR vs Distance of SMF-28 fiber with 1550 nm light')\n",
        "plt.show()"
      ],
